/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/spec",
    "types": ["jasmine"],
    "paths": {
      "@core": ["src/app/core"],
      "@shared": ["src/app/shared"],
      "@types": ["src/app/types"],
      "@apis": ["src/app/api"],
      "@features/configurator": ["src/app/features/car-configurator"]
    }
  },
  "include": ["src/**/*.spec.ts", "src/**/*.d.ts"]
}
